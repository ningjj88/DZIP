<segment>
  <xmlTag>
	  	<idxField name="RecordList" >
			<Group name="RecordMap">
				<xmlTag><String name="AcquCd"></String></xmlTag><!-- 受理机构标识-->	
				<xmlTag><String name="TranTime"></String></xmlTag><!-- 交易时间 -->	
		        <xmlTag><String name="SysTraceNum"></String></xmlTag><!-- 系统跟踪号 -->	
				<xmlTag><String name="AcctNo"></String></xmlTag><!-- 卡号-->
			    <xmlTag><String name="TranAMT"></String></xmlTag><!-- 金额 -->						
			    <xmlTag><String name="PDG1"></String></xmlTag><!-- -受理方收 -->	
				<xmlTag><String name="PDG2"></String></xmlTag><!-- 对方行收-->		
		    	<xmlTag><String name="TranCat"></String></xmlTag><!-- 交易类别-->
			</Group>
		</idxField>
	</xmlTag>
    <xmlTag><String name="TotalCount"></String></xmlTag><!--总共笔数 -->
	<xmlTag><String name="TotalPDG1"></String></xmlTag><!--受理方收总金额-->
	<xmlTag><String name="TotalPDG2"></String></xmlTag><!--对方行收总金额-->
</segment>