<segment>
  <xmlTag>
	  	<idxField name="RecordList" >
			<Group name="RecordMap">
			    <xmlTag><String name="OrgNo"></String></xmlTag><!-- 机构号 -->
				<xmlTag><String name="PersNo"></String></xmlTag><!-- 柜员号-->
				<xmlTag><String name="CashBoxNo"></String></xmlTag><!-- 柜员号-->
				<xmlTag><String name="AcquCd"></String></xmlTag><!-- 业务发生行代码-->		
				<xmlTag><String name="MsgType"></String></xmlTag><!-- 消息类型 -->	
				<xmlTag><String name="AcctNo"></String></xmlTag><!-- 卡号-->
				<xmlTag><String name="TranCat"></String></xmlTag><!-- 交易类别-->
				<xmlTag><String name="Credit_Debit"></String></xmlTag><!-- 交易类型 -->		
			    <xmlTag><String name="MerType"></String></xmlTag><!-- 业务分类标识 -->	
				<xmlTag><String name="TranAMT"></String></xmlTag><!-- 金额 -->	
				<xmlTag><String name="ATMFee"></String></xmlTag><!-- ATM手续费 -->		
				<xmlTag><String name="POSFee"></String></xmlTag><!-- POS手续费 -->	
				<xmlTag><String name="TranStat"></String></xmlTag><!-- 交易状态 -->	
				<xmlTag><String name="CancelID"></String></xmlTag><!--  撤销标识-->
				<xmlTag><String name="TranTime"></String></xmlTag><!-- 交易时间 -->
				<xmlTag><String name="SysTraceNum"></String></xmlTag><!-- 渠道跟踪号 -->		
				<xmlTag><String name="AuditFlag"></String></xmlTag><!-- 是否对账 -->				
			</Group>
		</idxField>
	</xmlTag>
</segment>