		<elementAlias name="ADD_DATA_PRI"  alias="bit57" /> <!--附加交易信息  -->
		<elementAlias name="IDENNUMBE"  alias="bit61" /> <!--证件编号-->
		<elementAlias name="RCVCODE"  alias="bit100" /> <!-- 接收方机构码-->
		<elementAlias name="NANSCRESERVE"  alias="bit121" /><!--  CUPS 保留-->
</mapAlias>